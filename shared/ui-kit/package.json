{
  "name": "@sn/ui-kit",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:changed": "node  ../../scripts/testChanged.js",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "lint:changed": "node  ../../scripts/lintChanged.js",
    "typecheck": "tsc --noEmit",
    "generate:icons": "svgr -d src/generated/icons src/icons && eslint \"src/generated/icons/**/*.tsx\" --fix",
    "generate": "yarn generate:icons"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "polished": "4.1.1",
    "react": "17.0.1",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@sn/config": "0.0.1",
    "@svgr/cli": "5.5.0",
    "@types/styled-system": "5.1.10"
  }
}
