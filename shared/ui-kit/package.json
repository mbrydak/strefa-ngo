    "icons:generate": "svgr -d src/icons/components src/icons/svg && eslint \"src/icons/components/**/*.tsx\" --fix"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "polished": "4.1.0",
    "react": "17.0.1",
    "styled-system": "5.1.5"
  },
  "devDependencies": {
    "@emotion/jest": "11.1.0",
    "@sn/config": "0.0.1",
    "@svgr/cli": "5.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.6.3",
    "@types/jest": "26.0.20",
    "@types/styled-system": "5.1.10",
    "jest": "26.6.3",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3"
  }
